<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Artist junct</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
</head>
<body>
    <h2>Music Website</h2>
    <a href="/artist" type="button" class="btn btn-primary btn-lg">Add data<a></a>

    <div class="container-fluid">
    <table class="table table-hover">
        <thead>
          <tr>
            <th scope="col">Id</th>
            <th scope="col">Genre</th>
            <th scope="col">Image</th>
            <!-- <th scope="col">Age</th>
            <th scope="col">City</th>
            <th scope="col">Action</th> -->
          </tr>
        </thead>
        <tbody>
            <% resSong.forEach((e) => { %> 
                <tr>
                <th scope="row"><%= e.id %></th>
                <td><%= e.genre %></td>
                <td><img height="200" width="200" class="img-thumbnail" src="<%= e.image %>" alt=""></td>
                <!-- <td><%= e.age %></td>
                <td><%= e.city %></td> -->
                
                <td><a href="/employees/delete/<%= e.id %>" type="button"  onclick="deleteAlert(`<%= e.name %>`)" class="btn btn-outline-primary">Delete</a>
                <a href="/employees/update/<%= e.id %>" type="button" class="btn btn-outline-primary">Update</a>
              </td>
                <%  })  %> 
                
            </tr>
        </tbody>
      </table>
  </div>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL"
    crossorigin="anonymous"></script>
</body>
</html>